<?xml version="1.0" encoding="UTF-8"?>
<proxy name="SampleManagerProxy" startOnLoad="true" transports="http https" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <conditionalRouter continueAfter="true">
                <conditionalRoute asynchronous="true" breakRoute="true">
                    <condition>
                        <match regex="/services/SampleManagerProxy/sample-batches" type="url"/>
                    </condition>
                    <target sequence="SampleManagerExtSEQ"/>
                </conditionalRoute>
                <conditionalRoute asynchronous="true" breakRoute="true">
                    <condition>
                        <match regex="/services/SampleManagerProxy.*" type="url"/>
                    </condition>
                    <target sequence="SampleManagerDsSEQ"/>
                </conditionalRoute>
            </conditionalRouter>
            <respond/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
</proxy>
